    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.16",
    "type-fest": "^4.26.1",
    "typescript": "^5.9.3",
    "vercel": "^48.5.0"
  },
  "packageManager": "pnpm@10.19.0"
