    "vercel:yolo": "vercel --build-env PNPM_HOME=$PNPM_HOME --build-env PNPM_VERSION=10.19.0 --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true --build-env VERCEL_TELEMETRY_DISABLED=1"
