    "blo": "^1.2.0",
    "next": "^16.0.0",
    "next-nprogress-bar": "^2.3.13",
    "next-themes": "^0.4.6",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "usehooks-ts": "^3.1.0",
    "viem": "^2.38.3",
    "wagmi": "^2.18.2",
    "zod": "^3.25.76",
