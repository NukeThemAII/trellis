# Next.js frontend environment configuration (example values)
# Copy to frontend/.env.local and populate for local development.

# Default vault/strategy addresses surfaced in the UI (optional)
NEXT_PUBLIC_DEFAULT_VAULT_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DEFAULT_STRATEGY_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DEFAULT_VAULT_SEPOLIA_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_DEFAULT_STRATEGY_SEPOLIA_ADDRESS=0x0000000000000000000000000000000000000000

# Chainlink feed for USD conversions (optional)
NEXT_PUBLIC_CHAINLINK_USD_FEED=0x0000000000000000000000000000000000000000
# Maximum age (in seconds) for Chainlink answers before they are treated as stale
NEXT_PUBLIC_CHAINLINK_STALE_AFTER=3600

# WalletConnect project ID required by RainbowKit
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=
